package vehiculos;

public class Vehiculo {
	String marca;
	int matricula;
	int id;
	int estadoDeposito;
	int capacidadDeposito;
	int km;
	
	public Vehiculo	(String marca, int matricula, int id, int estadoDeposito,int capacidadDeposito,int km) {
		this.marca=marca;
		this.matricula=matricula;
		this.id=id;
		this.estadoDeposito=estadoDeposito;	
		this.capacidadDeposito=estadoDeposito;
		this.km=km;
		
	}

}
